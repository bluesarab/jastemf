Let
	I_1 :=
		Let
			I_2 := I_1
		In I_2
	
	R_1 := R_2 + R_3
	R_2 := R_3
	R_3 := R_2
	V_1 := [R_1]
	
	V_2 :=
		Let
			V_3 := [2,V_4,3]
		In [1,V_3,4]
	V_4 := [1,2,[V_4,3],4,5]
	
	V_5 := [V_6]
	V_6 := [V_5]
	
	f_1(P) := [R_1 + P]
	f_2(P) := [R_2 + P]
	f_3(P) :=
		Let
			f_4(P) := [R_2]
		In [f_4(P)]
	f_5(P) :=
		Let
			f_6(P) := [f_3(P)]
		In [f_6(P)]
In [
	[
	I_1,
	R_1,
	R_2,
	R_3,
	V_1
	],
	
	[
	V_2,
	V_4,
	V_5,
	V_6
	],
	
	[
	f_1(1),
	f_2(1),
	f_3(1),
	f_5(1)
	]
	]
