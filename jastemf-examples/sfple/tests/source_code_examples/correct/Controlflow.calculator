Let
	T := true
	F := false
In [
	If T Then 1 Else 2 Fi,
	If F Then 1 Else 2 Fi,
	If
		If T Then F Else T Fi
		Then 1
		Else 2
	Fi,
	If
		If F Then F Else T Fi
		Then 1
		Else 2
	Fi
	]
