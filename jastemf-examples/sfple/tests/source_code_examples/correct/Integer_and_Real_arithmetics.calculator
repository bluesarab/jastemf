Let
	I := 6
	R := 1.5
	I_1 := 2
	R_1 := 2.0
In [
	[
	I + I,
	I - I,
	I * I,
	I / I
	],
	
	[
	I + R,
	I - R,
	I * R,
	I / R
	],
	
	[
	R + I,
	R - I,
	R * I,
	R / I
	],
	
	[
	R + R,
	R - R,
	R * R,
	R / R
	],
	
	[
	I_1 = 2,
	I_1 = 2.0,
	I_1 = 2.5,
	I_1 = 3,
	R_1 = 2,
	R_1 = 2.0,
	R_1 = 2.5,
	R_1 = 3,
	2 = I_1,
	2.0 = I_1,
	2.5 = I_1,
	3 = I_1,
	2 = R_1,
	2.0 = R_1,
	2.5 = R_1,
	3 = R_1
	],
	
	[
	I_1 # 2,
	I_1 # 2.0,
	I_1 # 2.5,
	I_1 # 3,
	R_1 # 2,
	R_1 # 2.0,
	R_1 # 2.5,
	R_1 # 3,
	2 # I_1,
	2.0 # I_1,
	2.5 # I_1,
	3 # I_1,
	2 # R_1,
	2.0 # R_1,
	2.5 # R_1,
	3 # R_1
	]
	]
