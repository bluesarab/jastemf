Procedure main() Begin
	Var i:Integer;
	Var r:Real;
	
	i := 3;
	r := 3.5;
	
	i + i;
	i + r;
	i - i;
	i - r;
	i * i;
	i * r;
	i / i;
	i / r;
	
	r + r;
	r + i;
	r - r;
	r - i;
	r * r;
	r * i;
	r / r;
	r / i;
	
	i * (r - i) + r;
End
