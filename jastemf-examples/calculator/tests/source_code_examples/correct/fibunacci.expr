Procedure main() Begin
	Procedure fibunacci(Var n:Integer) : Integer Begin
		Var result:Integer;
		If(n>1) 
			Then Begin 
				result := fibunacci(n-1) + fibunacci(n-2);
			End
		Else Begin 
			result := 1;
		End
		Fi;
		Writeln result;
		Return result;
	End;
	fibunacci(10);
End;