Procedure main() Begin
	Var i:Integer;
	Var r:Real;
	
	i := 3;
	r := 3.5;
	
	Write i + i;
	Write i + r;
	Write i - i;
	Write i - r;
	Write i * i;
	Write i * r;
	Write i / i;
	Write i / r;
	
	Write r + r;
	Write r + i;
	Write r - r;
	Write r - i;
	Write r * r;
	Write r * i;
	Write r / r;
	Write r / i;
	
	Write i * (r - i) + r;
End;
