Procedure main() Begin
	Var t:Boolean;
	Var f:Boolean;
	
	t := true;
	f := false;
	
	t;
	f;
	
	t = t;
	t = f;
	f = t;
	f = f;
	
	Not t;
	Not f;
	
	t And t;
	t And f;
	f And t;
	f And f;
	
	t Or t;
	t Or f;
	f Or t;
	f Or f;
	
	Not (f Or (t And t) And f);
End
