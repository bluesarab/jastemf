Procedure main() Begin
	Var x:Integer;
	Var y:Integer;
	
	x := 1;
	y := 2;
	
	Writeln x;
	Writeln y;
	
	Begin
		Writeln x;
		Writeln y;
		
		Var x:Boolean;
		x := true;
		
		Writeln x;
		Writeln y;
		
		Begin
			Writeln x;
			Writeln y;
			Var y:Boolean;
			y := true;
			Writeln x;
			Writeln y;
		End;
		
		Writeln x;
		Writeln y;
		
		Begin
			Writeln x;
			Writeln y;
			Var x:Real;
			Var y:Real;
			x := 1.0;
			y := 2.0;
			Writeln x;
			Writeln y;
		End;
		
		Writeln x;
		Writeln y;
	End;
	
	Writeln x;
	Writeln y;
End;
