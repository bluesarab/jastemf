Procedure main() Begin
	Var x:Integer;
	Var y:Integer;
	
	x := 1;
	y := 2;
	
	x;
	y;
	
	Begin
		x;
		y;
		
		Var x:Boolean;
		x := true;
		
		x;
		y;
		
		Begin
			x;
			y;
			Var y:Boolean;
			y := true;
			x;
			y;
		End;
		
		x;
		y;
		
		Begin
			x;
			y;
			Var x:Real;
			Var y:Real;
			x := 1.0;
			y := 2.0;
			x;
			y;
		End;
		
		x;
		y;
	End;
	
	x;
	y;
End
