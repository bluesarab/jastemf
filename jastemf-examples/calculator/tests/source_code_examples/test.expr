% Program for ad hoc testing from command line.
% Can be changed arbitrary; is not part of the test-suite.

Procedure main() Begin
	Write fibunacci(10);
End;

Procedure fibunacci(Var n:Integer):Integer Begin
	Var result:Integer;
	If (n > 1) 
		Then result := fibunacci(n-1) + fibunacci(n-2);
	Else
		result := 1;
	Fi;
	Return result;
End;
