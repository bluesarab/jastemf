Procedure main() Begin
	Var b:Boolean;
	Var i:Integer;
	Var r:Real;
	
	b := true;
	i := 1;
	r := 1.0;
	
	b = i;
	b = r;
	i = b;
	r = b;
	
	Not i;
	Not r;
	
	b And i;
	b And r;
	i And b;
	r And b;
	
	b Or i;
	b Or r;
	i Or b;
	r Or b;
	
	b Or (b And i = r) = i;
End
