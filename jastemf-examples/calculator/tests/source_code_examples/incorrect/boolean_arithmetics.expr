% @author C. Bürger
Procedure main() Begin
	Var b:Boolean;
	Var i:Integer;
	Var r:Real;
	
	b := true;
	i := 1;
	r := 1.0;
	
	Writeln b = i;
	Writeln b = r;
	Writeln i = b;
	Writeln r = b;
	
	Writeln Not i;
	Writeln Not r;
	
	Writeln b And i;
	Writeln b And r;
	Writeln i And b;
	Writeln r And b;
	
	Writeln b Or i;
	Writeln b Or r;
	Writeln i Or b;
	Writeln r Or b;
	
	Writeln b Or (b And i = r) = i;
End;
