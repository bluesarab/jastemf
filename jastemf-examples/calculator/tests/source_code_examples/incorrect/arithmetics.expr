Procedure main() Begin
	Var b:Boolean;
	Var i:Integer;
	Var r:Real;
	
	i := 3;
	r := 3.5;
	b := true;
	
	Writeln b + i;
	Writeln b + r;
	Writeln b - i;
	Writeln b - r;
	Writeln b * i;
	Writeln b * r;
	Writeln b / i;
	Writeln b / r;
	
	Writeln i + b;
	Writeln r + b;
	Writeln i - b;
	Writeln r - b;
	Writeln i * b;
	Writeln r * b;
	Writeln i / b;
	Writeln r / b;
	
	Writeln i * ((r + 2) - b) + r;
End;
