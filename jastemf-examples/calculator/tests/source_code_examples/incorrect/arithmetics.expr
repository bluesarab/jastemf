Procedure main() Begin
	Var b:Boolean;
	Var i:Integer;
	Var r:Real;
	
	i := 3;
	r := 3.5;
	b := true;
	
	b + i;
	b + r;
	b - i;
	b - r;
	b * i;
	b * r;
	b / i;
	b / r;
	
	i + b;
	r + b;
	i - b;
	r - b;
	i * b;
	r * b;
	i / b;
	r / b;
	
	i * ((r + 2) - b) + r;
End
