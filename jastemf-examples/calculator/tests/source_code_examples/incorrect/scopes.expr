Procedure main() Begin
	Var x:Integer;
	
	Begin
		Var z:Integer;
		z := 1; % The only correct statement of the program
	End;
	
	Writeln x;
	Writeln y;
	Writeln z;
	y := 1;
	z := 1;
	Writeln y;
	Writeln z;
	
	Begin
		Writeln x;
		Writeln y;
		Writeln z;
		y := 1;
		z := 1;
		Writeln y;
		Writeln z;
		
		Var x:Boolean;
		
		Writeln x;
		Writeln y;
		Writeln z;
		y := 1;
		z := 1;
		Writeln y;
		Writeln z;
		
		Begin
			Writeln x;
			Writeln y;
			Writeln z;
			y := 1;
			z := 1;
			Writeln y;
			Writeln z;
			
			Var x:Real;
			Var y:Real;
			
			Writeln x;
			Writeln y;
			Writeln z;
			z := 1;
			Writeln z;
		End;
		
		Writeln x;
		Writeln y;
		Writeln z;
		y := 1;
		z := 1;
		Writeln y;
		Writeln z;
	End;
	
	Writeln x;
	Writeln y;
	Writeln z;
	y := 1;
	z := 1;
	Writeln y;
