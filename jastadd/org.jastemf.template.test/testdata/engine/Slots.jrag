	public ASTNode ASTNode.copy() {
		try {
			return (ASTNode) clone();
		} catch (CloneNotSupportedException e) {
		}
		System.err.println("Error: Could not clone node of type "
				+ getClass().getName() + "!");
		return null;
	}
	
	public #LISTTYPE# #LISTTYPE#.copy() {
		try {
			return (#LISTTYPE#) clone();
		} catch (CloneNotSupportedException e) {}
		System.err.println("Error: Could not clone node of type "
				+ getClass().getName() + "!");
		return null;
	}
	
